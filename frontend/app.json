{
  "expo": {
    "name": "Flexi Flight",
    "slug": "flexi-flight",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.pthurow.frontend",
    "icon": "",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/splashscreen/splashscreen.png"
    },
    "splash": {
      "image": "./assets/splashscreen/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0D1E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi":true,
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "com.google.android.c2dm.permission.RECEIVE",
        "com.google.android.c2dm.permission.SEND"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0F0D1E"
      },
      "package": "com.pthurow.frontend"
    },
    "plugins": [
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c5287f58-26cb-41b8-b6cc-426b8dfd784c"
      }
    },
    "owner": "bababoys"
  }
}