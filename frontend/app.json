{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.pthurow.frontend",
    "icon": "",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscreen/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0D1E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/splashscreen/splash.png",
        "backgroundColor": "#0F0D1E"
      },
      "package": "com.pthurow.frontend"
    },
    "plugins": [
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9359b28e-2ffb-4084-8dc1-6d7bbbee352f"
      }
    }
  }
}
